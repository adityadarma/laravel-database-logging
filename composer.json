{
    "name": "adityadarma/laravel-database-logging",
    "description": "Logging activities database for laravel",
    "keywords": ["laravel", "database logging", "laravel logging"],
    "homepage": "https://github.com/adityadarma/laravel-database-logging",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aditya Darma",
            "email": "adhit.boys1@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
		"laravel/framework": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "AdityaDarma\\LaravelDatabaseLogging\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdityaDarma\\LaravelDatabaseLogging\\LaravelDatabaseLoggingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
